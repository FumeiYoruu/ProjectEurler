def div(x):
    #calculate the  cycle length
    pro = []
    #store the remainders that have existed
    nums = {}
    length = -1
    re = 10
    while re:
        an = int(re/x)
        pro.append(an)
        length+=1
        re -= an * x
        if not re in nums.keys():
            nums[re] = length
        else:
            #return if the remainder recurs
            return length - nums[re]
        re *= 10
        
    return 0
d = 2
ans = 0
num = 0
while d<1000:
    i = div(d)
    if (i > ans):
        ans = i
        num = d
    d+=1
print(num)
        
    

        
